{
	"name": "document-feeds",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "bun run --filter '*' dev",
		"dev:server": "cd packages/server && bun run dev",
		"dev:client": "cd packages/client && bun run dev",
		"deploy:client": "cd packages/client && bun run deploy",
		"deploy:server": "cd packages/client && bun run deploy"
	}
}
